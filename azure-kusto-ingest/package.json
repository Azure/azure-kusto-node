{
  "name": "azure-kusto-ingest",
  "version": "2.2.3",
  "description": "Azure Data Explorer Ingestion SDK",
  "main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">= 8.0.0"
  },
  "tags": [
    "azure",
    "kusto",
    "sdk"
  ],
  "keywords": [
    "node",
    "azure",
    "kusto"
  ],
  "scripts": {
    "build": "npm link ../azure-kusto-data && tsc -b",
    "prepublish": "npm run build",
    "example": "npm run build && node example.js",
    "lint": "npm run build && tslint --project tsconfig.json --quiet",
    "test": "npm run build && mocha --require ts-node/register",
    "e2e": "npm run build && mocha --require ts-node/register test/e2eTests/e2eTest.ts",
    "allTests": "npm run build && mocha --timeout 240000 --recursive --require ts-node/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Azure/azure-kusto-node.git",
    "directory": "azure-kusto-ingest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/storage-blob": "12.1.2",
    "@azure/storage-queue": "12.0.5",
    "@types/node": "^14.14.13",
    "@types/sinon": "^9.0.9",
    "@types/uuid": "^8.3.0",
    "@types/uuid-validate": "0.0.1",
    "azure-kusto-data": "latest",
    "moment": "^2.22.2",
    "request": "^2.88.0",
    "uuid": "^8.3.2",
    "uuid-validate": "0.0.3",
    "stream-to-array": "2.3.0",
    "stream-array": "1.1.2"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.0",
    "mocha": "^7.2.0",
    "sinon": "^7.2.3",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  }
}
