{
    "name": "azure-kusto-data",
    "version": "5.2.3",
    "description": "Azure Data Explorer Query SDK",
    "source": "src/index.ts",
    "scripts": {
        "clean": "rimraf dist/* dist-esm/* types/*",
        "build": "tsc -b"
    },
    "targets": {
        "browser": {
            "engines": {
                "browsers": "> 0.5%, not dead"
            },
            "context": "browser",
            "isLibrary": true,
            "outputFormat": "esmodule",
            "includeNodeModules": true
        },
        "node": {
            "engines": {
                "node": ">= 18.0.0"
            },
            "context": "node",
            "isLibrary": true,
            "outputFormat": "esmodule"
        }
    },
    "browser": {
        "fs": false,
        "os": false,
        "process": false,
        "stream-http": false,
        "https": false,
        "http": false,
        "crypto": false,
        "stream": false,
        "./dist-esm/src/connectionBuilder.js": "./dist-esm/src/connectionBuilder.browser.js"
    },
    "engines": {
        "node": ">= 18.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "tags": [
        "azure",
        "kusto",
        "sdk"
    ],
    "keywords": [
        "node",
        "azure",
        "kusto"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Azure/azure-kusto-node.git",
        "directory": "azure-kusto-data"
    },
    "files": [
        "dist-esm/src/*.js",
        "dist-esm/src/*.js.map",
        "types/src/*.d.ts",
        "tsconfig.tsbuildinfo",
        "dist-esm/src/wellKnownKustoEndpoints.json"
    ],
    "author": "",
    "license": "ISC",
    "homepage": "https://github.com/Azure/azure-kusto-node/blob/master/azure-kusto-data/README.md",
    "devDependencies": {
        "assert": "^2.0.0",
        "rimraf": "^3.0.2"
    },
    "dependencies": {
        "@azure/core-auth": "^1.6.0",
        "@azure/core-util": "^1.7.0",
        "@azure/identity": "^3.3.2",
        "@types/uuid": "^8.3.4",
        "axios": "^1.6.0",
        "follow-redirects": "^1.15.1",
        "https-browserify": "^1.0.0",
        "stream-http": "^3.2.0",
        "uuid": "^8.3.2"
    },
    "gitHead": "f8a5dae26d6d2ca2ab8b95953bb9b88a02e8e35d"
}
