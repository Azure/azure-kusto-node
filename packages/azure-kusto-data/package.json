{
  "name": "azure-kusto-data",
  "version": "5.2.3",
  "description": "Azure Data Explorer Query SDK",
  "types": "dist/node/azure-kuato-data.d.ts",
  "module": "dist/node/index.mjs",
  "type": "module",
  "scripts": {
    "bundle-node": "nx exec -- node ../../esbuild.mjs --node",
    "bundle-browser": "nx exec -- node ../../esbuild.mjs",
    "types-node": "nx exec -- tsc --project tsconfig.json",
    "types-browser": "nx exec -- tsc --project tsconfig.browser.json"
  },
  "browser": {
    "./src/connectionBuilder.ts": "./src/connectionBuilder.browser.ts"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "tags": [
    "azure",
    "kusto",
    "sdk"
  ],
  "keywords": [
    "node",
    "azure",
    "kusto"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Azure/azure-kusto-node.git",
    "directory": "azure-kusto-data"
  },
  "files": [
    "dist-esm/src/*.js",
    "dist-esm/src/*.js.map",
    "types/src/*.d.ts",
    "tsconfig.tsbuildinfo",
    "dist-esm/src/wellKnownKustoEndpoints.json"
  ],
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/Azure/azure-kusto-node/blob/master/azure-kusto-data/README.md",
  "devDependencies": {
    "assert": "^2.0.0",
    "rimraf": "^3.0.2",
    "nx": "18.0.6"
  },
  "dependencies": {
    "@azure/identity": "^3.3.2",
    "@types/uuid": "^8.3.4",
    "axios": "^1.6.0",
    "follow-redirects": "^1.15.1",
    "https-browserify": "^1.0.0",
    "stream-http": "^3.2.0",
    "uuid": "^8.3.2"
  },
  "gitHead": "f8a5dae26d6d2ca2ab8b95953bb9b88a02e8e35d",
  "nx": {
    "targets": {
      "bundle-node": {
        "outputs": [
          "{projectRoot}/dist/index.mjs"
        ]
      },
      "bundle-browser": {
        "outputs": [
          "{projectRoot}/dist/browser/index.mjs"
        ]
      },
      "types-node": {
        "outputs": [
          "{projectRoot}/dist/node/types"
        ]
      },
      "types-browser": {
        "outputs": [
          "{projectRoot}/dist/browser/types"
        ]
      },
      "build": {
        "executor": "nx:noop",
        "dependsOn": [
          "bundle-node",
          "bundle-browser",
          "types-node",
          "types-browser"
        ]
      }
    }
  }
}
